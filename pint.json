{
    "preset": "laravel",
    "exclude": [
        "bootstrap",
        "config",
        "public",
        "resources",
        "storage"
    ],
    "rules": {
        "final_class": true,
        "void_return": true,
        "declare_strict_types": true,
        "ordered_imports": {
            "sort_algorithm": "length"
        },
        "php_unit_method_casing": {
            "case": "snake_case"
        }
    }
}
